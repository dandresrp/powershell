{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "blocks": [
    {
      "alignment": "left",
      "segments": [
        {
          "foreground": "#98C379",
          "properties": {
            "postfix": "",
            "prefix": "",
            "text": "➜"
          },
          "style": "plain",
          "type": "text"
        },
        {
          "foreground": "#56B6C2",
          "properties": {
            "style": "folder"
          },
          "style": "plain",
          "type": "path"
        },
        {
          "foreground": "#D0666F",
          "properties": {
            "branch_icon": "",
            "postfix": "<#5FAAE8>)</>",
            "prefix": "<#5FAAE8>git:(</>",
            "template": "{{ .HEAD }}"
          },
          "style": "plain",
          "type": "git"
        },
        {
          "foreground": "#BF616A",
          "properties": {
            "postfix": "",
            "template": "✗"
          },
          "style": "plain",
          "type": "exit"
        }
      ],
      "type": "prompt"
    }
  ],
  "final_space": true
}
